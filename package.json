{
    "name": "argb_controller",
    "productName": "A RGB Controller",
    "version": "1.0.1",
    "description": "A minimal Electron application with node serialport",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "install": "electron-rebuild",
        "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/icon.icns --prune=true --out=release-builds",
        "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
        "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/512x512.png --prune=true --out=release-builds"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:johnny-five-io/electron-serialport.git"
    },
    "author": "GitHub",
    "license": "CC0-1.0",
    "devDependencies": {
        "electron": "^6.0.7",
        "electron-installer-dmg": "^3.0.0",
        "electron-packager": "^14.0.5",
        "electron-rebuild": "^1.8.5"
    },
    "dependencies": {
        "serialport": "^7.1.5"
    }
}
